<?xml version="1.0" encoding="UTF-8"?>
<cronentries>
    <cron>
        <url>/cron/getbalance</url>
        <target>1</target>
        <description>Get Balance every 2 min</description>
        <schedule>every 24 hours</schedule>
        <retry-parameters>
            <min-backoff-seconds>2.5</min-backoff-seconds>
            <max-doublings>5</max-doublings>
        </retry-parameters>
    </cron>
</cronentries>